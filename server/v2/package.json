    "koa-router": "^12.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/koa": "^2.13.9",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-router": "^7.4.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
