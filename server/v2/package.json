{
  "name": "ts-koa-postgres-api",
  "version": "1.0.0",
  "description": "TypeScript Koa API with PostgreSQL connection",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch '**/*.ts' --exec 'ts-node' server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
